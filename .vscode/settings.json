{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[css]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[graphql]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "emmet.showExpandedAbbreviation": "never",
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#dc85b2",
    "activityBar.background": "#dc85b2",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#d0eab5",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#dc85b2",
    "statusBar.background": "#d15d99",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#c33880",
    "statusBarItem.remoteBackground": "#d15d99",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#d15d99",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#d15d9999",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#d15d99"
}
